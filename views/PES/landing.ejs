                                <h4 class="text-center mb-3"><%= event.date %></h4>
                                <% if(event.time){ %>
                                <h5 class="text-center mb-3"><%= event.time %></h5>
                                <% } %>
                                <p class="mx-3 mx-md-0">
                                    <%= event.description %>
                                </p>
                                <% if(event.reglink){ %>
                                <div class="row justify-content-center">
                                    <a class="btn btn-success mx-3" href="<%= event.reglink %>">Register Now!</a>
